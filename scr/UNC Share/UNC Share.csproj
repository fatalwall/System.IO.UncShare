<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net47;net462</TargetFrameworks>
    <RootNamespace>vshed.IO</RootNamespace>
    <AssemblyName>vshed.IO.UncShare</AssemblyName>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://vshed.us/vshed.IO.UncShare/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/fatalwall/vshed.IO.UncShare</RepositoryUrl>
    <PackageTags>UNC Network Share</PackageTags>
    <NeutralLanguage></NeutralLanguage>
    <Copyright>Copyright © Peter Varney 2019, 2020</Copyright>
    <Description>Library for accessing UNC Shares with an alternative identity</Description>
    <Company>Peter Varney</Company>
    <Product>UNC Share</Product>
    <Version>1.0.2</Version>
    <Authors>Peter Varney</Authors>
    <PackageReleaseNotes>Modified to add support for Net 4.6.2 per request</PackageReleaseNotes>
    <AssemblyVersion>1.0.2</AssemblyVersion>
    <VersionSuffix>$([System.DateTime]::UtcNow.ToString(mmff))</VersionSuffix>
    <FileVersion>$(AssemblyVersion).$(VersionSuffix)</FileVersion>
  </PropertyGroup>

</Project>
